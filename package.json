{
  "name": "zaml",
  "version": "0.12.0",
  "description": "Fast, type-checked, zero-dependency configuration.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && browserify index.browser.js -o dist/index.browser.js",
    "start": "tsc --watch",
    "test": "ospec",
