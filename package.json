    "prepublishOnly": "npm test"
  },
  "keywords": [
    "config",
    "parser",
    "format",
    "schema",
    "validation"
  ],
  "author": "https://github.com/gilbert",
  "repository": "github:gilbert/zaml",
  "license": "MIT",
  "devDependencies": {
    "browserify": "^16.2.3",
    "ospec": "^3.0.1",
    "typescript": "^3.1.6"
  }
}
